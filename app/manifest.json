{
    "name": "YouTube Anti Translate",
    "version": "1.5",
    "description": "A small extension to disable YT video titles autotranslation.",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["src/background.js"]
    }],
    "browser_action": {
        "default_popup": "pages/popup.html"
    },
    "permissions": [
        "storage", "tabs"
    ],
    "icons": {
        "48": "icon48.png"
    }

}
